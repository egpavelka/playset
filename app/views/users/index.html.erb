<% provide(:title, 'member directory') %>
<section class="content-container">
  <div class="page-header">members</div>
  <%= will_paginate %>
  <ul class="users">
    <% @users.each do | user | %>
      <li>
        <%= link_to user.username, user %>
      </li>
    <% end %>
  </ul>
  <%= will_paginate %>
</section>
